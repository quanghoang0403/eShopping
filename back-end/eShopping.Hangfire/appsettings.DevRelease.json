{
  "Logging": {
    "LogLevel": {
      "Default": "__Logging.LogLevel.Default__",
      "Microsoft": "__Logging.LogLevel.Microsoft__"
    }
  },
  "ConnectionStrings": {
    "Hangfire": "__ConnectionStrings.Hangfire__"
  },
  "Hangfire": {
    "User": "__Hangfire.User__",
    "Pass": "__Hangfire.Pass__"
  },
  "HttpClientSetting": {
    "Admin": "__HttpClientSetting.Admin__",
    "POS": "__HttpClientSetting.POS__"
  },
  "InternalAuthenticate": {
    "Username": "__InternalAuthenticate.Username__",
    "Password": "__InternalAuthenticate.Password__"
  },
  "JobSettings": [
    {
      "JobName": "CheckBranchesExpirationTimeJob",
      "CronExpression": "__JobSettings.0.CronExpression__", // Every 5 minutes
      "EndPoint": "__JobSettings.0.EndPoint__"
    },
    {
      "JobName": "ResetAccumulatedPointJob",
      "CronExpression": "__JobSettings.1.CronExpression__", // At 12:00 AM
      "EndPoint": "__JobSettings.1.EndPoint__"
    },
    {
      "JobName": "SendEmailCampaignJob",
      "CronExpression": "__JobSettings.2.CronExpression__", // At 12:00 AM
      "EndPoint": "__JobSettings.2.EndPoint__"
    },
    {
      "JobName": "SendNotificationCampaignJob",
      "CronExpression": "__JobSettings.3.CronExpression__", // At 12:00 AM
      "EndPoint": "__JobSettings.3.EndPoint__"
    },
    {
      "JobName": "AutoComfirmOrderJob",
      "CronExpression": "__JobSettings.4.CronExpression__", // Every minutes
      "EndPoint": "__JobSettings.4.EndPoint__"
    }
  ],
  "Serilog": {
    "EnableSqlCommandText": "__Serilog.EnableSqlCommandText__",
    "MinimumLevel": {
      "Default": "__Serilog.MinimumLevel.Default__",
      "Override": {
        "Microsoft": "__Serilog.MinimumLevel.Override.Microsoft__"
      }
    },
    "Using": [
      "Serilog.Sinks.ApplicationInsights"
    ],
    "WriteTo": [
      {
        "Args": {
          "instrumentationKey": "__Serilog.WriteTo.0.Args.instrumentationKey__",
          "roleInstance": "__Serilog.WriteTo.0.Args.roleInstance__",
          "roleName": "__Serilog.WriteTo.0.Args.roleName__",
          "telemetryConverter": "GoFoodBeverage.Loging.Serilog.OperationTelemetryConverter, GoFoodBeverage.Loging"
        },
        "Name": "ApplicationInsights"
      },
      {
        "Name": "Console"
      }
    ]
  }
}